{
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-new": 0,
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0,
    "import/newline-after-import": 0,
    "no-plusplus": 0,
    "radix": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "no-prototype-builtins": 0,
    "no-use-before-define": [
      2,
      {
        "functions": false,
        "classes": false
      }
    ],
    "no-restricted-syntax": [
      2,
      "LabeledStatement",
      "WithStatement"
    ]
  },
  "globals": {
    "Config": true,
    "Helper": true
  }
}
